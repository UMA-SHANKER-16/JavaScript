let score="33"
console.log(typeof score)
 let number=Number(score)
 console.log(typeof number)

 let count="33abc"
 let num=Number(count)
 console.log(typeof num)
 console.log(num)
 //33abc its type is a nnumber but its a not an number NaN
// "33"=>33
// "33abc" => NaN
// true => 1
// false => 0


let isLoggedIn ="121"
